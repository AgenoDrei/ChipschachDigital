<div id="impressum" class="modal fade" role="dialog">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
                <h4 class="modal-title"><%=strings.modals.impressum.title%></h4>
            </div>
            <div class="modal-body">
                <a href="http://www.schach-fuer-kids.de/">
                    <img src="../img/logoSm.png" height="150px">
                </a>
                <h3><%=strings.modals.impressum.disclaim%></h3>
		        <%=strings.modals.impressum.txt%>
                <ul>
                    <% strings.modals.impressum.contributors.forEach(function(contributor) { %>
                    <li><%=contributor%></li>
                    <% }); %>
                </ul>
            </div>
            <div class="modal-footer">
                <%=strings.modals.impressum.footer.txts[0]%>: <a href="http://www.schach-fuer-kids.de"><%=strings.modals.impressum.footer.info_link%></a> <br>
                <%=strings.modals.impressum.footer.txts[1]%>: <a href="mailto:chipschach@schach-fuer-kids.de?Subject=Anfrage" target="_top"><%=strings.modals.impressum.footer.email%></a> <br>
		        <%=strings.modals.impressum.footer.version%>: 2.5.0
            </div>
        </div>
    </div>
</div>
